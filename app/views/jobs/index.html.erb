<table>

  <thead>
  <tr>
    <th>Title:</th>
    <th>Description:</th>
    <th>Company:</th>
    <th>Url:</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%=job.title %></td>
        <td><%=job.description %></td>
        <td><%=job.company %></td>
        <td><%=job.url %></td>
        <td> <%= link_to 'Show', job_path(job) %> </td>
        <td> <%= link_to 'Edit ', edit_job_path(job) %> </td>
        <td> <%= link_to 'Destroy', job, method: :delete,
                         data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>


  <p>
    <%= link_to "New job", new_job_path %>
  </p>


</table>